<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>HttpSessionAttributeListener</description>
        <listener-class>kic.admin.SessionAttributeListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>AddRoom</servlet-name>
        <servlet-class>kic.admin.AddRoom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>kic.admin.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>kic.admin.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DoIt</servlet-name>
        <servlet-class>kic.admin.DoIt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>kic.admin.Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddRoom</servlet-name>
        <url-pattern>/add_room.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/dashboard.action</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DoIt</servlet-name>
        <url-pattern>/job_tracker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout.action</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
